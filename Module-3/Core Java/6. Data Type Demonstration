public class DataTypeDemo {
    public static void main(String[] args) {
        // Declare and assign values to variables of different primitive types
        int age = 25;
        float temperature = 36.6f;
        double pi = 3.1415926535;
        char grade = 'A';
        boolean isJavaFun = true;

        // Display the values using System.out.println()
        System.out.println("Integer (int) value: " + age);
        System.out.println("Float (float) value: " + temperature);
        System.out.println("Double (double) value: " + pi);
        System.out.println("Character (char) value: " + grade);
        System.out.println("Boolean (boolean) value: " + isJavaFun);
    }
}

